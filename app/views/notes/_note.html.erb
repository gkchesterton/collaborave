<li>

  <span> <%= note.content %> </span>
  <span class="timestamp">
    Posted <%= time_ago_in_words(note.created_at) %> ago by <%= note.collaborator.name %>. 
    
  </span>
  <% if current_user == note.collaborator %>
  <%= button_to "Delete",  note, method: :delete, class: "btn btn-mini btn-danger", confirm: "Are you sure that you wish to delete this note?" %>
  <% end %>
</li>
